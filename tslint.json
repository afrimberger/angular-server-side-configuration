{
  "defaultSeverity": "error",
  "extends": ["tslint-plugin-prettier", "tslint-config-prettier", "tslint:recommended"],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": [true, "no-public"],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "ordered-imports": [
      true,
      {
        "grouped-imports": true
      }
    ],
    "quotemark": [true, "single"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  },
  "rulesDirectory": []
}
